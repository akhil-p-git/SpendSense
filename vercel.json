{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(users|recommendations|profile|consent|what-if|operator|eval|history|session|transactions)/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/assets/(.*)",
      "dest": "ui-react/dist/assets/$1"
    },
    {
      "src": "/(.*\\..+)",
      "dest": "ui-react/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "ui-react/dist/index.html"
    }
  ],
  "outputDirectory": "ui-react/dist",
  "functions": {
    "api/index.py": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}
