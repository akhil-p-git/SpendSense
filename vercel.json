{
  "version": 2,
  "buildCommand": "cd ui-react && npm install && npm run build",
  "outputDirectory": "ui-react/dist",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(users|recommendations|profile|consent|what-if|operator|eval|history|session|transactions)/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}
